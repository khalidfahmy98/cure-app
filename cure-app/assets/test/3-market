    display:inline-block;
    line-height:50px;
    margin:0 20px;
}
.nav-bar ul li a.nav-list{
    color:white;
    font-weight:bold;
    font-size:15px;
    text-decoration:none;
}
.nav-bar ul li a:hover{
    color:#8ed8e6;
}
.nav-bar .checkbtn{
    color:white;
    font-size:30px;
    line-height:80px;
    float:right;
    margin-right:40px;
    cursor:pointer;
    display:none;
}
.nav-bar #check{
    display:none;
}
