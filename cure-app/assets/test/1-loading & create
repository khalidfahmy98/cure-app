/*Start Loading Screen*/
.loading-overflow{
    top:0;
    left:0;
    width:100%;
    height:100vh;
    background-size:cover;
    background-repeat:no-repeat;
    position:absolute;
    z-index:9999;
}
.loading-screen{
    background-image:url("../imgs/loading.png.png");
    top:0;
    left:0;
    width:100%;
    height:100vh;
    background-size:cover;
    background-repeat:no-repeat;
    position:absolute;
}
.loading-screen .cure-animation{
    width:100px;
    position:relative;
    top:-10px;
    display:block;
    margin:auto;
    -webkit-animation:moving 5s infinite ease-in-out;
    -moz-animation:moving 5s infinite ease-in-out;
    -o-animation:moving 5s infinite ease-in-out;
    animation:moving 5s infinite ease-in-out;
}
@keyframes moving{

    50%{
        top:200px;
        bottom:100px;
    }
}
/*End Loading Screen*/
/*Start Navbar*/
